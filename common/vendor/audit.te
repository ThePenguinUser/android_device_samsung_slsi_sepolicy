#============= gpsd ==============
allow gpsd system_data_file:file lock;
allow gpsd system_data_file:fifo_file { create read };
allow gpsd system_data_file:fifo_file open;

#============= hal_gnss_default ==============
allow hal_gnss_default system_data_file:fifo_file { read setattr write };
allow hal_gnss_default system_data_file:fifo_file open;

#============= system_app ==============
allow system_app sysfs:file getattr;

#============= toolbox ==============
allow toolbox rootfs:dir open;
allow toolbox rootfs:dir setattr;

#============= vold ==============
allow vold rootfs:dir write;
allow vold rootfs:dir add_name;
allow vold rootfs:dir create;
